# API Keys for AI Providers
# Copy this file to .env and fill in your actual API keys

# OpenAI Configuration
OPENAI_API_KEY=your-openai-api-key-here
# Optional: OPENAI_BASE_URL=https://api.openai.com/v1
# Optional: OPENAI_PROMPT_MODEL=gpt-4
# Optional: OPENAI_IMAGE_MODEL=dall-e-3

# Google Gemini Configuration  
GEMINI_API_KEY=your-gemini-api-key-here
# Alternative: GOOGLE_API_KEY=your-google-api-key-here
# Optional: GEMINI_BASE_URL=https://generativelanguage.googleapis.com/v1beta
# Optional: GEMINI_MODEL=gemini-2.0-flash-exp
# Optional: GEMINI_IMAGE_MODEL=imagen-3.0-generate-001

# Ollama Configuration (Local)
OLLAMA_BASE_URL=http://localhost:11434
OLLAMA_MODEL=llama3.1

# LM Studio Configuration (Local)
LMSTUDIO_BASE_URL=http://localhost:1234/v1
LMSTUDIO_MODEL=local-model

# Optional: Global timeout settings (in seconds)
# TIMEOUT=60